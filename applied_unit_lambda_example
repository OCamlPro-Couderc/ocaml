`functor_unit_body` = 
       Lprim(Pfield 0, [Lprim(Pgetglobal `compunit_name`, [])])

Llet(Strict, env0_id, initial_env,
             Llet(Strict, env1_id,
                          Lapply(mod_prim "add_functor_arg",
                                 [`name_of_argument`,    ; Argument of the functor 
                                  `value_of_argument`,   ; Module name
                                   env0_id],
                                 Location.none)
                          Lapply(`functor_unit_body`, [env1_id], Location.none)))
                                  
